<Window x:Class="StroyCompany.EditObjectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StroyCompany"
        mc:Ignorable="d"
        Title="EditObjectWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Name:"/>
            <TextBox x:Name="nameTextBox" Margin="0,5,0,10"/>
            
            <TextBlock Text="Address:"/>
            <TextBox x:Name="addressTextBox" Margin="0,5,0,10"/>
            
            <TextBlock Text="Type:"/>
            <TextBox x:Name="typeTextBox" Margin="0,5,0,10"/>
            
            <TextBlock Text="Group Name:"/>
            <TextBox x:Name="groupNameTextBox" Margin="0,5,0,10"/>
            
            <TextBlock Text="Cost:"/>
            <TextBox x:Name="costTextBox" Margin="0,5,0,10"/>
            
            <Button Content="Edit" Click="EditButton_Click" HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Window>
